<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>Servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>manageKeyServlet</servlet-name>
        <servlet-class>Servlet.ManageKeyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>activationServlet</servlet-name>
        <servlet-class>Servlet.ActivationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkCodeServlet</servlet-name>
        <servlet-class>Servlet.CheckCodeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getScriptServlet</servlet-name>
        <servlet-class>Servlet.GetScriptServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>scriptServlet</servlet-name>
        <servlet-class>Servlet.SetScriptServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>manageKeyServlet</servlet-name>
        <url-pattern>/manageKey</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>activationServlet</servlet-name>
        <url-pattern>/activation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkCodeServlet</servlet-name>
        <url-pattern>/checkCode</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getScriptServlet</servlet-name>
        <url-pattern>/getScript</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>scriptServlet</servlet-name>
        <url-pattern>/setScript</url-pattern>
    </servlet-mapping>

</web-app>